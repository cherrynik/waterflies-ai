{
  "name": "@fireflies/server-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@fireflies/server"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/server-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@fireflies/server:build",
          "@fireflies/server:serve"
        ]
      }
    }
  }
}
